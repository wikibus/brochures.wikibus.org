{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.Loggly"
        ],
        "MinimumLevel": "Debug",
        "WriteTo": {
            "Name": "Loggly",
            "Args": {
                "tags": "wikibus-sources",
                "endpointHostName": "logs-01.loggly.com",
                "isEnabled": "true"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithExceptionDetails",
            "WithThreadId"
        ]
    },
    "Logging": {
        "IncludeScopes": false,
        "Database": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "wikibus": {
        "baseUrl": "https://sources.wikibus.org/",
        "apiUrl": "https://wikibus-sources.herokuapp.com/"
    }
}
